hm_d_lcmd: behavior_dev_lcmd {
    compatible = "zmk,behavior-hold-tap";
    label = "hm_d_lcmd";
    #binding-cells = <2>;
    tapping-term-ms = <300>;
    quick-tap-ms = <200>;
    flavor = "tap-unless-interrupted";
    global-quick-tap;
    bindings = <&kp>, <&mp_d_lao>;
};

