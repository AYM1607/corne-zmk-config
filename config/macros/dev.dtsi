m_bs: macro_brackets_square {
    compatible = "zmk,behavior-macro";
    label = "[░]";
    #binding-cells = <0>;
    bindings =
        <&kp LBKT &kp RBKT>,
        <&kp LEFT>;
};

m_bc: macro_brackets_curly {
    compatible = "zmk,behavior-macro";
    label = "{░}";
    #binding-cells = <0>;
    bindings =
        <&kp LBRC &kp RBRC>,
        <&kp LEFT>;
};

m_br: macro_brackets_round {
    compatible = "zmk,behavior-macro";
    label = "(░)";
    #binding-cells = <0>;
    bindings =
        <&kp LPAR &kp RPAR>,
        <&kp LEFT>;
};

m_ba: macro_brackets_angular {
    compatible = "zmk,behavior-macro";
    label = "<░>";
    #binding-cells = <0>;
    bindings =
        <&kp LT &kp GT>,
        <&kp LEFT>;
};

