    bindings =
        <&macro_press   &kp RALT>,
        <&macro_tap     &kp KP_N0 &kp KP_N1 &kp KP_N6 &kp KP_N1>,
        <&macro_release &kp RALT>;
};

m_d_bo: macro_boolean_operator {
    compatible = "zmk,behavior-macro";
