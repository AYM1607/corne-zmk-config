    compatible = "zmk,behavior-macro";
    label = "--";
    #binding-cells = <0>;
    bindings =
        <&kp MINUS &kp MINUS>;
};

m_d_a: macro_dev_arrow {
    compatible = "zmk,behavior-macro";
    label = "=>";
    #binding-cells = <0>;
