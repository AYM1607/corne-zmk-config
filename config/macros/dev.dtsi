m_d_cqm: macro_close_question_mark {
    compatible = "zmk,behavior-macro";
    label = "¿";
    #binding-cells = <0>;
    bindings =
        <&macro_press   &kp RALT>,
        <&macro_tap     &kp KP_N1 &kp KP_N9 &kp KP_N1>,
        <&macro_release &kp RALT>;
};

