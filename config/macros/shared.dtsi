to_BAS: to_BAS_layer {
    compatible = "zmk,behavior-macro";
    label = "to_BAS";
    #binding-cells = <0>;
    bindings =
        <&to BAS &kp F_BAS>;
};

to_DEV: to_DEV_layer {
    compatible = "zmk,behavior-macro";
    label = "to_DEV";
    #binding-cells = <0>;
    bindings =
        <&to DEV &kp F_DEV>;
};

to_AXN: to_AXN_layer {
    compatible = "zmk,behavior-macro";
