mo_AXN_to_BAS: momentary_AXN_to_BAS {
    compatible = "zmk,behavior-macro";
    label = "mo_AXN_to_BAS";
    #binding-cells = <0>;
    wait-ms = <0>;
    tap-ms = <0>;
    bindings
        = <&macro_press &mo AXN>
        , <&macro_tap &kp F_AXN>
        , <&macro_pause_for_release>
        , <&macro_release &mo AXN>
        , <&macro_tap &kp F_BAS>;
};

