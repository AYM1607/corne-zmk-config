        , <&macro_tap &kp F_AYN>
        , <&macro_pause_for_release>
        , <&macro_release &mo AYN>
        , <&macro_tap &kp F_DEV>;
};

mo_STG_to_DEF: momentary_STG_to_DEF {
    compatible = "zmk,behavior-macro";
    label = "mo_STG_to_DEF";
    #binding-cells = <0>;
