cw_aON: cw_aON {
    compatible = "zmk,behavior-macro";
    label = "cw_aON";
    #binding-cells = <0>;
    bindings =
        <&kp LC(H_aCWR) &cw>;
};

cw_aOFF: cw_wOFF {
    compatible = "zmk,behavior-macro";
    label = "cw_aOFF";
    #binding-cells = <0>;
    bindings =
        <&kp LA(H_aCWR) &kp K_CANCEL>;
};