mo_aAXN_to_aBAS: momentary_aAXN_to_aBAS {
    compatible = "zmk,behavior-macro";
    label = "mo_aAXN_to_aBAS";
    #binding-cells = <0>;
    wait-ms = <0>;
    tap-ms = <0>;
    bindings
        = <&macro_press &mo aAXN>
        , <&macro_tap &kp F_aAXN>
        , <&macro_pause_for_release>
        , <&macro_release &mo aAXN>;
};

