hm_ab_lcmd: behavior_android_base_lcmd {
    compatible = "zmk,behavior-hold-tap";
    label = "hm_ab_lcmd";
    #binding-cells = <2>;
    tapping-term-ms = <HP_TAPPING>;
    quick-tap-ms = <HP_QUICK_TAP>;
    flavor = "hold-preferred";
    global-quick-tap;
    bindings = <&kp>, <&mp_ai_a>;
    hold-trigger-key-positions = KEYS_RIGHT;
};

