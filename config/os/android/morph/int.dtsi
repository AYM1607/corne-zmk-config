mp_ai_a: morph_android_a {
    compatible = "zmk,behavior-mod-morph";
    label = "a";
    #binding-cells = <0>;
    mods = <(MOD_LALT)>;
    bindings = <&kp A>, <&kp LC(A)>;
    keep-mods = <(MOD_LALT)>;
};

