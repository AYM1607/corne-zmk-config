// action: **
m_wd_eo: macro_win_dev_exponentiation_operator {
    compatible = "zmk,behavior-macro";
    label = "m_wd_eo";
    #binding-cells = <0>;
    wait-ms = <400>;
    bindings
        = <&none &kp LALT> /* ALT fix for VSCode */
        , <&macro_wait_time 100>
        , <&kp ASTRK &kp ASTRK>;
};

